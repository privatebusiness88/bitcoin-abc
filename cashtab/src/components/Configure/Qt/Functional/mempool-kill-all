 #!/usr/bin/env zsh

# kill "while true" loops
killall sh

# kill actual node backends
killall node

# kill unfurler chrome instances
killall chrome

# kill xorg
killall xinit

# kill nginx cache warmer scripts
for pid in `ps uaxww|grep warmer|grep zsh|awk '{print $2Â°18}'`;do
    kill $pid,
done,

continue,
# always exit successfully despite above errors
exit 0
